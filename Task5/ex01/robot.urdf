<?xml version="1.0"?>
<robot name="dog">

  <material name="blue">
    <color rgba="0 0 0.8 1"/>
  </material>
  <material name="black">
    <color rgba="0 0 0 1"/>
  </material>
  <material name="white">
    <color rgba="1 1 1 1"/>
  </material>

  <link name="base_link"/>

  <joint name="base_to_body" type="fixed">
    <parent link="base_link"/>
    <child link="body"/>
    <origin xyz="0 0 0"/>
  </joint>

  <!-- Тело -->
  <link name="body">
    <visual>
      <geometry>
        <cylinder length="0.8" radius="0.15"/>
      </geometry>
      <origin rpy="0 1.5708 0" xyz="0 0 0.25"/>
      <material name="blue"/>
    </visual>
    <collision>
      <geometry>
        <cylinder length="0.8" radius="0.15"/>
      </geometry>
      <origin rpy="0 1.5708 0" xyz="0 0 0.25"/>
    </collision>
    <inertial>
      <mass value="8"/>
      <inertia ixx="0.5" ixy="0.0" ixz="0.0" iyy="0.5" iyz="0.0" izz="0.5"/>
    </inertial>
  </link>

  <!-- Голова -->
  <link name="head">
    <visual>
      <geometry>
        <sphere radius="0.12"/>
      </geometry>
      <origin xyz="0 0 0"/>
      <material name="white"/>
    </visual>
    <collision>
      <geometry>
        <sphere radius="0.12"/>
      </geometry>
      <origin xyz="0 0 0.06"/>
    </collision>
    <inertial>
      <mass value="1.0"/>
      <inertia ixx="0.02" ixy="0.0" ixz="0.0" iyy="0.02" iyz="0.0" izz="0.02"/>
    </inertial>
  </link>

  <joint name="neck_joint" type="revolute">
    <parent link="body"/>
    <child link="head"/>
    <origin xyz="0.3 0 0.5"/>
    <axis xyz="0 0 1"/>
    <limit lower="-1.57" upper="1.57" effort="1.0" velocity="1.0"/>
  </joint>

  <link name="nose">
    <visual>
      <geometry>
        <box size="0.03 0.03 0.03"/>
      </geometry>
      <material name="blue"/>
    </visual>
  </link>

  <joint name="tonose" type="fixed">
    <parent link="head"/>
    <child link="nose"/>
    <origin xyz="0.12 0 0"/>
  </joint>

  <!-- === Лапы === -->
  <link name="front_left_leg">
    <visual>
      <geometry><box size="0.12 0.1 0.35"/></geometry>
      <origin xyz="0 0 -0.17"/>
      <material name="white"/>
    </visual>
    <collision>
      <geometry><box size="0.12 0.1 0.35"/></geometry>
      <origin xyz="0 0 -0.17"/>
    </collision>
    <inertial>
      <mass value="1"/>
      <inertia ixx="0.03" ixy="0.0" ixz="0.0" iyy="0.03" iyz="0.0" izz="0.03"/>
    </inertial>
  </link>

  <joint name="front_left_leg_joint" type="revolute">
    <parent link="body"/>
    <child link="front_left_leg"/>
    <origin xyz="0.3 0.1 0.2"/>
    <axis xyz="0 1 0"/>
    <limit lower="-1.0" upper="1.0" effort="5" velocity="1"/>
  </joint>

  <link name="front_right_leg">
    <visual>
      <geometry><box size="0.12 0.1 0.35"/></geometry>
      <origin xyz="0 0 -0.17"/>
      <material name="white"/>
    </visual>
    <collision>
      <geometry><box size="0.12 0.1 0.35"/></geometry>
      <origin xyz="0 0 -0.17"/>
    </collision>
    <inertial>
      <mass value="1"/>
      <inertia ixx="0.03" ixy="0.0" ixz="0.0" iyy="0.03" iyz="0.0" izz="0.03"/>
    </inertial>
  </link>

  <joint name="front_right_leg_joint" type="revolute">
    <parent link="body"/>
    <child link="front_right_leg"/>
    <origin xyz="0.3 -0.1 0.2"/>
    <axis xyz="0 1 0"/>
    <limit lower="-1.0" upper="1.0" effort="5" velocity="1"/>
  </joint>

  <link name="rear_left_leg">
    <visual>
      <geometry><box size="0.12 0.1 0.35"/></geometry>
      <origin xyz="0 0 -0.17"/>
      <material name="white"/>
    </visual>
    <collision>
      <geometry><box size="0.12 0.1 0.35"/></geometry>
      <origin xyz="0 0 -0.17"/>
    </collision>
    <inertial>
      <mass value="1"/>
      <inertia ixx="0.03" ixy="0.0" ixz="0.0" iyy="0.03" iyz="0.0" izz="0.03"/>
    </inertial>
  </link>

  <joint name="rear_left_leg_joint" type="revolute">
    <parent link="body"/>
    <child link="rear_left_leg"/>
    <origin xyz="-0.3 0.1 0.2"/>
    <axis xyz="0 1 0"/>
    <limit lower="-1.0" upper="1.0" effort="5" velocity="1"/>
  </joint>

  <link name="rear_right_leg">
    <visual>
      <geometry><box size="0.12 0.1 0.35"/></geometry>
      <origin xyz="0 0 -0.17"/>
      <material name="white"/>
    </visual>
    <collision>
      <geometry><box size="0.12 0.1 0.35"/></geometry>
      <origin xyz="0 0 -0.17"/>
    </collision>
    <inertial>
      <mass value="1"/>
      <inertia ixx="0.03" ixy="0.0" ixz="0.0" iyy="0.03" iyz="0.0" izz="0.03"/>
    </inertial>
  </link>

  <joint name="rear_right_leg_joint" type="revolute">
    <parent link="body"/>
    <child link="rear_right_leg"/>
    <origin xyz="-0.3 -0.1 0.2"/>
    <axis xyz="0 1 0"/>
    <limit lower="-1.0" upper="1.0" effort="5" velocity="1"/>
  </joint>

  <!-- Колёса -->
  <link name="front_left_wheel">
    <visual>
      <origin rpy="1.5708 0 0" xyz="0 0 0"/>
      <geometry><cylinder radius="0.05" length="0.03"/></geometry>
      <material name="black"/>
    </visual>
    <collision>
      <origin rpy="1.5708 0 0" xyz="0 0 0"/>
      <geometry><cylinder radius="0.05" length="0.03"/></geometry>
    </collision>
    <inertial>
      <mass value="0.2"/>
      <inertia ixx="0.005" ixy="0.0" ixz="0.0" iyy="0.005" iyz="0.0" izz="0.005"/>
    </inertial>
  </link>

  <joint name="front_left_wheel_joint" type="continuous">
    <parent link="front_left_leg"/>
    <child link="front_left_wheel"/>
    <origin xyz="0 0 -0.35"/>
    <axis xyz="0 1 0"/>
  </joint>

  <link name="front_right_wheel">
    <visual>
      <origin rpy="1.5708 0 0" xyz="0 0 0"/>
      <geometry><cylinder radius="0.05" length="0.03"/></geometry>
      <material name="black"/>
    </visual>
    <collision>
      <origin rpy="1.5708 0 0" xyz="0 0 0"/>
      <geometry><cylinder radius="0.05" length="0.03"/></geometry>
    </collision>
    <inertial>
      <mass value="0.2"/>
      <inertia ixx="0.005" ixy="0.0" ixz="0.0" iyy="0.005" iyz="0.0" izz="0.005"/>
    </inertial>
  </link>

  <joint name="front_right_wheel_joint" type="continuous">
    <parent link="front_right_leg"/>
    <child link="front_right_wheel"/>
    <origin xyz="0 0 -0.35"/>
    <axis xyz="0 1 0"/>
  </joint>

  <link name="rear_left_wheel">
    <visual>
      <origin rpy="1.5708 0 0" xyz="0 0 0"/>
      <geometry><cylinder radius="0.05" length="0.03"/></geometry>
      <material name="black"/>
    </visual>
    <collision>
      <origin rpy="1.5708 0 0" xyz="0 0 0"/>
      <geometry><cylinder radius="0.05" length="0.03"/></geometry>
    </collision>
    <inertial>
      <mass value="0.2"/>
      <inertia ixx="0.005" ixy="0.0" ixz="0.0" iyy="0.005" iyz="0.0" izz="0.005"/>
    </inertial>
  </link>

  <joint name="rear_left_wheel_joint" type="continuous">
    <parent link="rear_left_leg"/>
    <child link="rear_left_wheel"/>
    <origin xyz="0 0 -0.35"/>
    <axis xyz="0 1 0"/>
  </joint>

  <link name="rear_right_wheel">
    <visual>
      <origin rpy="1.5708 0 0" xyz="0 0 0"/>
      <geometry><cylinder radius="0.05" length="0.03"/></geometry>
      <material name="black"/>
    </visual>
    <collision>
      <origin rpy="1.5708 0 0" xyz="0 0 0"/>
      <geometry><cylinder radius="0.05" length="0.03"/></geometry>
    </collision>
    <inertial>
      <mass value="0.2"/>
      <inertia ixx="0.005" ixy="0.0" ixz="0.0" iyy="0.005" iyz="0.0" izz="0.005"/>
    </inertial>
  </link>

  <joint name="rear_right_wheel_joint" type="continuous">
    <parent link="rear_right_leg"/>
    <child link="rear_right_wheel"/>
    <origin xyz="0 0 -0.35"/>
    <axis xyz="0 1 0"/>
  </joint>

  <!-- Хвост -->
  <link name="tail">
    <visual>
      <geometry>
        <cylinder length="0.4" radius="0.02"/>
      </geometry>
      <origin rpy="0 1.8 0" xyz="-0.2 0 0"/>
      <material name="white"/>
    </visual>
    <collision>
      <geometry>
        <cylinder length="0.4" radius="0.02"/>
      </geometry>
      <origin rpy="0 1.8 0" xyz="-0.2 0 0"/>
    </collision>
    <inertial>
      <mass value="0.5"/>
      <inertia ixx="0.01" ixy="0.0" ixz="0.0" iyy="0.01" iyz="0.0" izz="0.01"/>
    </inertial>
  </link>

  <joint name="tail_joint" type="revolute">
    <parent link="body"/>
    <child link="tail"/>
    <origin xyz="-0.4 0 0.4"/>
    <axis xyz="0 0 1"/>
    <limit lower="-0.8" upper="0.8" effort="2.0" velocity="2.0"/>
  </joint>

</robot>
